@article{ WOS:000247095200032,
Author = {Huang, H. and Wu, J. Y. and Wu, J. H.},
Title = {Heavy metal monitoring using bivalved shellfish from Zhejiang coastal
   waters, East China Sea},
Journal = {ENVIRONMENTAL MONITORING AND ASSESSMENT},
Year = {2007},
