@article{ WOS:A1986D420500002,
Author = {AMIARDTRIQUET, C and BERTHET, B and METAYER, C and AMIARD, JC},
Title = {CONTRIBUTION TO THE ECOTOXICOLOGICAL STUDY OF CADMIUM, COPPER AND ZINC
   IN THE MUSSEL MYTILUS-EDULIS .2. EXPERIMENTAL-STUDY},
Journal = {MARINE BIOLOGY},
Year = {1986},
Volume = {92},
Number = {1},
Pages = {7-13},
Abstract = {A regulation of internal levels of some essential metals has been
   observed in various animals, whereas the bioaccumulation of several
   non-essential metals parallels their overloads in water. In the mussel
   Mytilus edulis L., we have attempted to determine if such a phenomenon
   exists by comparing the patterns of accumulation of copper and zinc vs
   cadmium. With this aim, mussels collected in the Bay of Bourgneuf
   (France) in November 1983 were exposed to these metals for 16 d. At
   external levels of zinc as high as 100 .mu.g l-1, mussels were able to
   maintain a normal concentration in all groups of organs for 4 d. The
   ability of mussels to limit the bioaccumulation of copper and zinc
   varied from organ to organ, and decreased with higher levels of
   contamination and longer periods of exposure. In contrast, at the lowest
   experimental concentration and the lowest period of exposure, a
   significant increase of cadmium in mussel tissues was generally
   observed. Even at the highest sub-lethal doses, the levels of copper and
   zinc in mussel tissues were not much higher than the natural levels
   (contaminated:background ratios = 2.3 to 6.1), whereas the
   bioaccumulation of cadmium was less well restricted
   (contaminated:background ratios = 136 to 192). The use of mussels as a
   bioindicator of pollution seems doubtful for essential metals,
   particularly as regards short-term pollution, since the levels of these
   trace elements in the organisms are largely independent of their
   concentration in the ambient seawater.},
