@article{ WOS:A1981LZ57200008,
Author = {WONG, MH and CHOY, CK and LAU, WM and CHEUNG, YH},
Title = {HEAVY-METAL CONTAMINATION OF THE PACIFIC OYSTERS (CRASSOSTREA-GIGAS)
   CULTURED IN DEEP-BAY, HONG-KONG},
Journal = {ENVIRONMENTAL RESEARCH},
Year = {1981},
Volume = {25},
Number = {2},
Pages = {302-309},
Abstract = {Samples of Pacific oysters, C. gigas, cultured in Deep Bay, Hong Kong,
   were collected for analysis of various heavy metal contents: Fe, Cu, Pb,
   Cd, Mn, Cr and Zn. The gill had the highest metal contents, followed by
   the mantle, viscera, with the smallest amount being observed in the
   adductor muscle. All metal contents were within the range reported for
   the same species in areas where heavy metal pollution was already
   recognized as a problem. A long-term monitoring program measuring the
   contents of various metals in seawater, sediment and oysters is
   essential.},
