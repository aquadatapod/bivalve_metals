@article{ WOS:000173583000004,
Author = {Giusti, L and Zhang, H},
Title = {Heavy metals and arsenic in sediments, mussels and marine water from
   Murano (Venice, Italy)},
Journal = {ENVIRONMENTAL GEOCHEMISTRY AND HEALTH},
