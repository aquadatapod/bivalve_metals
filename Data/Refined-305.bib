Title = {Assessment of Electrolytes and Metals Profile of the Faro Lake (Capo
   Peloro Lagoon, Sicily, Italy) and Its Impact on <i>Mytilus
   galloprovincialis</i>},
Journal = {CHEMISTRY \& BIODIVERSITY},
Year = {2018},
