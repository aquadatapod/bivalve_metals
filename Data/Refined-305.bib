Title = {Seasonal accumulation of trace elements in native Mediterranean mussels
   (<i>Mytilus galloprovincialis</i> Lamarck, 1819) collected in the Calich
   Lagoon (Sardinia, Italy)},
Journal = {ENVIRONMENTAL SCIENCE AND POLLUTION RESEARCH},
Year = {2021},
