@article{ WOS:000312412700007,
Author = {Topi, Teuta and Bani, Aida and Malltezi, Jamarber and Sulce, Sulejman},
Title = {HEAVY METALS IN SOIL, SEDIMENTS, MUSSELS, AND WATER FROM BUTRINTI LAGOON
   (ALBANIA)},
Journal = {FRESENIUS ENVIRONMENTAL BULLETIN},
Year = {2012},
Volume = {21},
Number = {10A, SI},
Pages = {3042-3051},
Abstract = {The Lagoon of Butrinti, is located in the south of Albania, and it
   represents an important ecological area that is known for its mussels
   farming. Heavy metals enter the lagoons through several pathways, mainly
   due to the pedo-geological processes as well as human activity. This
   study aims to quantify the origin, the spatial distribution and the
   level of pollution caused by heavy metals in soil, sediments, water and
   mussels of Butrinti lagoon. The soil samples were taken at two different
   locations in the proximity of the Butrinti lagoon. The water, sediments
   and mussels samples have been collected from seven selected sites during
   August 2011. The results showed that the concentration of Pb, Cu, and Cd
   in some of the samples in question was above the normal standards of the
   parameters for the respective soil, reflecting the impact of
   humanactivity while the high Cr content was spotted due to the natural
   climate and weather related process. The results also showed that Pb
   (SedS 4, 6, 7, EF=2.17-3.17), Cd (SedS 7, EF=1.47) and Cr (SedS7,
   EF=1.61) are enriched in the sediment. In addition the level of Pb (SS6)
   and Cr (SS6, SS7) in the underground water was above the standards
   defined by EU-2008. This study shows that concentrations of Cd in
   mussels over 0.3 mg kg(-1) are at a critical limit and need to be
   continuously monitored. This study suggests that the sources of this
   contamination are the anthropogenic input, previous agricultural
   activities, contributions of the tributaries, and direct impact from the
   activity in the urban area of Ksamil.},
