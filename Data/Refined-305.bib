@article{ WOS:000220990900022,
Author = {Tomazelli, AC and Martinelli, LA and Avelar, WEP and de Camargo, PB and
   Fostier, AH and Ferraz, ESB and Krug, FJ and Júnior, DS},
Title = {Biomonitoring of Pb and Cd in two impacted watersheds in southeast
   Brazil, using the freshwater mussel <i>Anodontites trapesialis</i>
   (Lamarck, 1819) (Bivalvia: Mycetopodidae) as a biological monitor},
Journal = {BRAZILIAN ARCHIVES OF BIOLOGY AND TECHNOLOGY},
Year = {2003},
Volume = {46},
Number = {4},
Pages = {673-684},
Month = {DEC},
Abstract = {Studies were carried out to investigate the contamination of Piracicaba
   and Mogi-Guacu River basins (Sao Paulo State, Southeastern, Brazil)for
   heavy metals (Cd and Pb), using the limnic bivalve Anodontites
   trapesialis as a biological Monitor. The results showed that the
   concentrations of Pb were higher than the control group in both basins,
   showing the bioavailability of this non-essential element in the basins.
   The concentrations were higher in the Mogi-Guacu than in the Piracicaba
   basin, and in the slightly contaminated sites in both basins. There was
   no correlation between the degree of human impact and Cd and Pb
   concentrations, it was not possible to infer about concentrations of
   these heavy metals in the bivalves based only in a broad evaluation of
   human impact.},
