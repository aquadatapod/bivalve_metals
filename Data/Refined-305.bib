@article{ WOS:000237149500020,
Author = {Gifford, SP and MacFarlane, GR and O'Connor, WA and Dunstan, RH},
Title = {Effect of the pollutants lead, zinc, hexadecane and octocosane on total
   growth and shell growth in the Akoya pearl oyster, <i>Pinctada
   imbricata</i>},
