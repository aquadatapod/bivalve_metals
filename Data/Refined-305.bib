@article{ WOS:001217836500001,
Author = {Vidyalakshmi, D. and Yesudas, Aneena and Sivan, Gopika and Prakash, E.
   Akhil and Priyaja, P.},
Title = {Heavy metal accumulation analysis using bivalve, sponge, sea urchin, and
   gastropod species as bioindicators},
Journal = {MARINE POLLUTION BULLETIN},
Year = {2024},
Volume = {202},
Month = {MAY},
Abstract = {A comparative assessment of heavy metal accumulation potential in four
   distinct marine benthic bioindicators: the bivalve Perna perna, the
   sponge Callyspongia fibrosa, the sea urchin Tripneustes gratilla, and
   the gastropod Purpura bufo were conducted. These organisms were
   collected from the same location, and the concentration of ten heavy
   metals was analyzed in water, sediment and various body parts of the
   organisms. The bioaccumulation potential was evaluated using the
   bio-water accumulation factor and bio-sediment accumulation factor.
   There was significant variation in the bioaccumulation potential of each
   organism with respect to different metals. The sponge proved to be a
   reliable indicator of Cd with a highest concentration of 2.60 mu g/g.
   Sea urchin accumulated high concentrations of Cr (16.98 mu g/g) and Pb
   (4.80 mu g/g), whereas Cu was predominant (21.05 mu g/g) in gastropod,
   followed by bivalve (17.67 mu g/g). The concentration of metals in hard
   parts was found to be lower than in the tissues.},
