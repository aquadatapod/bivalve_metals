@article{ WOS:000084252300005,
Author = {Cope, WG and Bartsch, MR and Rada, RG and Balogh, SJ and Rupprecht, JE
   and Young, RD and Johnson, DK},
Title = {Bioassessment of mercury, cadmium, polychlorinated biphenyls, and
   pesticides in the upper Mississippi river with zebra mussels
   (<i>Dreissena polymorpha</i>)},
Journal = {ENVIRONMENTAL SCIENCE \& TECHNOLOGY},
Year = {1999},
Volume = {33},
