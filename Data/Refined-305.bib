@article{ WOS:A1981KY48400005,
Author = {MENASVETA, P and CHEEVAPARANAPIWAT, V},
Title = {HEAVY-METALS, ORGANOCHLORINE PESTICIDES AND PCBS IN GREEN MUSSELS,
   MULLETS AND SEDIMENTS OF RIVER MOUTHS IN THAILAND},
Journal = {MARINE POLLUTION BULLETIN},
Year = {1981},
Volume = {12},
Number = {1},
Pages = {19-25},
Abstract = {Levels of Pb, Zn, Cu, Cd and Hg and certain organochlorines including
   DDT and PCB {[}polychlorinated biphenyls] were determined in green
   mussels, mullets and bottom sediments of mouths of Mae Klong, Ta Chin,
   Chao Phraya and Ban Pakong Rivers {[}Thailand]. The 4 river mouths were
   contaminated by heavy metals to some extent. Pb contamination in
   mussels, mullets and sediments was obvious at values significantly
   higher than those previously reported. Contamination of the other metals
   in mussels and mullets was within acceptable limits, except for Hg in
   Chao Phraya River mouth which approached the acceptable limit.
   Organochlorines, especially DDT and its metabolites, were detectable in
   some samples of mussels, mullet and sediments. PCB were detectable in
   some samples of mussels and mullets but not in sediments. Levels of
   these organochlorines were comparable to baseline levels.},
