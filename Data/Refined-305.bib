@article{ WOS:A1982NL76700006,
Author = {VIARENGO, A and PERTICA, M and MANCINELLI, G and PALMERO, S and
   ZANICCHI, G and ORUNESU, M},
Title = {EVALUATION OF GENERAL AND SPECIFIC STRESS INDEXES IN MUSSELS COLLECTED
   FROM POPULATIONS SUBJECTED TO DIFFERENT LEVELS OF HEAVY-METAL POLLUTION},
Journal = {MARINE ENVIRONMENTAL RESEARCH},
Year = {1982},
Volume = {6},
Number = {3},
Pages = {235-243},
Abstract = {The rate of protein synthesis was significantly decreased in various
   tissues of mussels gathered from a coastal area characterized by a high
   level of pollution when compared with animals collected in the reference
   unpolluted area. Such a decrease may represent a general stress index.
   Additional biological parameters such as amino acid uptake and RNA
   synthesis were similarly decreased in mussels collected from the
   polluted area. In the digestive gland of animals sampled from the heavy
   metal polluted area, the level of low MW thionein-like Cu-binding
   proteins was 3 times higher than in the digestive gland of controls. The
   increase in the level of metallothionein-like proteins in the digestive
   gland of the mussels represented a specific stress index directly
   related to the presence in the seawater of high concentrations of heavy
   metals.},
