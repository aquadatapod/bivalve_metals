@article{ WOS:001106790800001,
Author = {Subasinghe, M. M. and Jinadasa, B. K. K. K. and Navarathne, A. N. and
   Jayakody, S.},
Title = {Potential health risk assessment of selected metal concentrations of
   Indian backwater oyster,<i> (Crassostrea</i><i> madrasensis)</i> in Sri
   Lanka},
Journal = {MARINE POLLUTION BULLETIN},
Year = {2023},
Volume = {196},
Month = {NOV},
Abstract = {The study was conducted to assess the health risks of selected metals in
   cultured and wild Crassostrea madrasensis in Sri Lanka over 13 months.
   Metal concentrations varied monthly, and Zn and Cu concentrations
   significantly varied between cultured and wild oysters. The mean metal
   concentrations (mg kg(-1)) were accumulated in order of Zn (1220.60) >
   Mg (496.97) > Fe (356.30) > Cu (114.95) > Pb (23.20) > Cd (6.15) > As
   (3.09) > Hg (0.12). The metal concentrations were well below the
   standard guidelines given by international standards except for Pb, Cd,
   As and Zn. The weekly intakes of Hg, Cu, Fe, and Zn from C.madrasensis
   were below the Provisional Tolerable Weekly Intake while Cd exceeded the
   established limit. The calculated non-carcinogenic and carcinogenic risk
   indices for Hg, As, Pb and Zn were within safe levels and the values for
   Cd exceeded the limit revealing carcinogenic results with long-term
   consumption. Hence, creating proper awareness and a suitable depuration
   system ensures human health and supports export-oriented markets.},
