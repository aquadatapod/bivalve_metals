@article{ WOS:000518864500004,
Author = {Li, Peng and Huang, Yu-Ying and Zeng, Jun and Lin, Zheng-Zhong and
   Huang, Zhi-Yong},
Title = {Health risk assessment of heavy metals in shellfish collected from
   Fujian, China},
Journal = {HUMAN AND ECOLOGICAL RISK ASSESSMENT},
Year = {2020},
Volume = {26},
Number = {3},
Pages = {621-635},
Month = {MAR 15},
Abstract = {With industrialization and human activities, shellfish may be
   contaminated by various pollutants such as heavy metals. This study aims
   at the concentrations of As, Cr, Mn, Ni, Cu, Zn, Cd, Sn, Sb and Pb in
   shellfish collected from Fujian of China, and the risk of heavy metals
   in shellfish on human health based on target hazard quotients (THQ).
   Results showed that the THQ values of the elements were far below 1,
   except for As with an average value of 1.148 in razor clam. No
   detrimental health effects of heavy metals on humans health was observed
   by daily consumption of mussel and oyster, but the exposed population to
   short-necked clam, scallop and razor clam might experience
   noncarcinogenic health risks because each of the total THQ values was
   above 1 for the three shellfish.},
