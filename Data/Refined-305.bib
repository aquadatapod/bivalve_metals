@article{ WOS:A1976CK09900008,
Author = {PHILLIPS, DJH},
Title = {COMMON MUSSEL MYTILUS-EDULIS AS AN INDICATOR OF POLLUTION BY ZINC,
   CADMIUM, LEAD AND COPPER .2. RELATIONSHIP OF METALS IN MUSSEL TO THOSE
   DISCHARGED BY INDUSTRY},
Journal = {MARINE BIOLOGY},
Year = {1976},
Volume = {38},
Number = {1},
Pages = {71-80},
Abstract = {Concentrations of Zn, Cd, Pb and Cu were determined in whole soft parts
   of M. edulis sampled in Port Phillip Bay and Western Port Bay, Victoria,
   Australia. The mussels were sampled according to procedures suggested by
   previous studies to eliminate the effects of natural environmental
   variables. Results of the analytical studies were compared to data on
   the quantities of trace metals discharged by industry into the
   catchments of each bay. This allowed an evaluation of the indicator
   ability of the mussel without the need for multiple analyses of water
   samples. The mussel apparently can act as an efficient time-integrated
   indicator of Zn, Cd and Pb over a wide variety of environmental
   conditions. Although additional variables may remain to be investigated,
   the mussel is recommended as an alternative to the analysis of water and
   sediments. With further development this organism may also be useful for
   monitoring industrial effluent characteristics at the discharge site,
   allowing rapid, reliable and inexpensive control of water quality. The
   mussel is of doubtful use as an indicator of Cu; other indicator types
   such as macroalgae may be better-suited to the study of this element.},
