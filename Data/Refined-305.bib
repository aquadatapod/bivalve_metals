@article{ WOS:000082174400031,
Author = {Miller, BS},
Title = {Mussels as biomonitors of point and diffuse sources of trace metals in
   the Clyde Sea Area, Scotland},
Journal = {WATER SCIENCE AND TECHNOLOGY},
