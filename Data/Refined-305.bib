@article{ WOS:A1985AEF3500003,
Author = {CAIN, DJ and LUOMA, SN},
Title = {COPPER AND SILVER ACCUMULATION IN TRANSPLANTED AND RESIDENT CLAMS
   (MACOMA-BALTHICA) IN SOUTH SAN-FRANCISCO BAY},
Journal = {MARINE ENVIRONMENTAL RESEARCH},
Year = {1985},
Volume = {15},
Number = {2},
Pages = {115-135},
Abstract = {Accumulation of Cu and Ag by soft tissues of the deposit-feeding clam M.
   balthica was less than half in clams transplanted to a contaminated area
   than in clams native to that area. During a period of tissue growth, the
   transplants retained 50\% and 90\%, respectively, of the net Cu and Ag
   accumulated, but loss of metals from soft tissue by the resident
   population equalled net accumulation. Cu accumulation in the transplants
   did not occur during some periods when increases in the metal body 
   burden of the resident population indicated that environmental exposures
   were high. The difference in metal accumulation of the 2 groups of clams
   may be the result of past environmental exposures. The results
   illustrate some limitations of using transplants as indicators of
   pollution events or of pollutant impact upon resident populations.},
